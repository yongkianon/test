<?xml version="1.0"?>
<configuration>

  <property>
    <name>yarn.resourcemanager.ha.enabled</name>
    <value>true</value>
  </property>

  <property>
    <name>yarn.resourcemanager.connect.retry-interval.ms</name>
    <value>5000</value>
  </property>

  <property>
    <name>yarn.resourcemanager.cluster-id</name>
    <value>yarn-res-cluster</value>
  </property>

  <property>
    <name>yarn.resourcemanager.ha.rm-ids</name>
    <value>rm01,rm02</value>
  </property>

  <property>
    <name>yarn.resourcemanager.hostname.rm01</name>
    <value>rm01</value>
  </property>

  <property>
    <name>yarn.resourcemanager.webapp.address.rm01</name>
    <value>rm01:8088</value>
  </property>

  <property>
    <name>yarn.resourcemanager.hostname.rm02</name>
    <value>rm02</value>
  </property>

  <property>
    <name>yarn.resourcemanager.webapp.address.rm02</name>
    <value>rm02:8088</value>
  </property>

  <property>
    <name>yarn.resourcemanager.zk-address</name>
    <value>zk01:2181,zk02:2181,zk03:2181</value>
  </property>

</configuration>

